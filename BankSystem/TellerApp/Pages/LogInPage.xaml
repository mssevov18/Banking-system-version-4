<Page x:Class="TellerApp.Pages.LogInPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TellerApp.Pages"
      xmlns:uc="clr-namespace:WpfLibrary.Models.UserControls;assembly=WpfLibrary"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LogInPage">

    <StackPanel Background="White">
        <uc:LogInForm x:Name="loginform"/>
        <Button x:Name="SubmitButton" Click="SubmitButton_Click" Content="Log in" Height="50" Margin="10 10 10 10"/>
    </StackPanel>
</Page>
